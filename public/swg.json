  "/api/openai/gemma-7b-it": {
    "post": {
      "summary": "Chat with Gemma Made With Cloudflare",
      "description": "Gemma-7b-it is so perfect. Start a conversation with it by providing the necessary inputs.",
      "tags": [
        "Artificial Intelligence"
      ],
      "requestBody": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "text": {
                  "type": "string",
                  "description": "Your question or input text for Gemma.",
                  "example": "Hai Aku Nana"
                },
                "prompt": {
                  "type": "string",
                  "description": "The specific instruction or behavior you want Gemma to follow.",
                  "example": "Kamu Sekarang Menjadi Pemandu Belajar Ku"
                }
              },
              "required": ["text", "prompt"]
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "Success",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "boolean",
                    "description": "Indicates whether the request was successful.",
                    "example": true
                  },
                  "author": {
                    "type": "string",
                    "description": "The name of the AI author.",
                    "example": "Celestial"
                  },
                  "result": {
                    "type": "string",
                    "description": "The response generated by Gemma-7b-it.",
                    "example": "Hello, I am Gemma. How can I assist you today?"
                  }
                }
              }
            }
          }
        },
        "400": {
          "description": "Bad Request - Missing or invalid parameters in the request body.",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "error": {
                    "type": "string",
                    "description": "Details about the error.",
                    "example": "Missing required parameters: 'text' or 'prompt'."
                  }
                }
              }
            }
          }
        }
      }
    }
  }, 
